{
  "name": "folks-finance-js-sdk",
  "version": "0.3.2",
  "description": "The official JavaScript SDK for the Folks Finance Protocol",
  "main": "dist/cjs/index.js",
  "types": "dist/cjs/types/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/types/index.d.ts",
        "default": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/cjs/types/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Folks-Finance/folks-finance-js-sdk.git"
  },
  "keywords": [
    "Folks Finance",
    "Algorand"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json && mv dist/esm/index.js dist/esm/index.mjs",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "prepack": "npm run build",
    "docs": "typedoc src/index.ts --options typedoc.config.json --tsconfig tsconfig.cjs.json",
    "example": "ts-node"
  },
  "dependencies": {
    "algosdk": "^1.18.0"
  },
  "devDependencies": {
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.21",
    "mocha": "^9.2.1",
    "ts-node": "^10.5.0",
    "typedoc": "^0.23.21",
    "typescript": "^4.9.3"
  },
  "author": "Folks Finance",
  "license": "MIT"
}
